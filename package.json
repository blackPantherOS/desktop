    "gulp-filter": "^4.0.0",
    "gulp-gzip": "^1.2.0",
    "gulp-jsbeautifier": "^2.0.3",
    "gulp-jshint": "^2.0.0",
    "gulp-load-plugins": "^1.2.0",
    "gulp-stylus": "^2.3.1",
    "gulp-tar": "^1.8.0",
    "guppy-pre-commit": "^0.4.0",
    "jshint": "^2.9.1",
    "jshint-stylish": "^2.1.0",
    "nib": "^1.1.0",
